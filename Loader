local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()

local Window = Rayfield:CreateWindow({
	Name = "Verny Hub Loader",
	LoadingTitle = "Verny Hub Loader",
	LoadingSubtitle = "by VernyFX,AdamFX",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = false,
        	Invite = "sirius", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "Verny Hub",
		Subtitle = "Key System",
		Note = "Join the discord (discord.gg/Vt6WpNb5)",
		FileName = "SiriusKey",
		SaveKey = false,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "324fsduugdf435omsdfmuiewrmok423"
	}
})

local Tab = Window:CreateTab("Script Loader", 4483362458) -- Title, Image

local Button = Tab:CreateButton({
	Name = "Load Script",
	Interact = "Load It!!!",
	Callback = function()
		if game.PlaceId == 8540346411 then
			wait(1)
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Vernyfx/RCX/main/jajaxd'))()
		
		
		elseif game.PlaceId == 9498006165 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/ts/main/asd",true))();
		

		elseif game.PlaceId == 2629642516 then
		wait(1)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/btc/main/asd",true))();
		
		
		
		elseif game.PlaceId == 8750997647 then
		wait(1)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/tsx/main/asd",true))();
		
		
		elseif game.PlaceId == 10598587051 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/animeclicker/main/jajaxd4",true))();
		
		
		elseif game.PlaceId == 6875469709 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/Strongest-Punxh/main/jajaxd3",true))();
			
			elseif game.PlaceId == 9170761016 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/magiccli/main/jajaxd5",true))();
			
						elseif game.PlaceId == 11162791099 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/SandboyHatchers/main/Hub",true))();
			
						elseif game.PlaceId == 11445923563 then
			wait(1)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Vernyfx/Onefruit/main/jajaxd45",true))();

		end
		wait(1)
		Rayfield:Destroy()
	end,
})
